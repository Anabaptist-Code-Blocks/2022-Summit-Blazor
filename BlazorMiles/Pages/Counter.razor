@page "/counter"

<h1>Counter</h1>

<p>Current abc count: @currentCount</p>


<button class="btn btn-primary" @onclick="IncrementCount">Click me</button>

<FetchData counter="currentCount"/>

@code {
    private int currentCount { get; set; } = 0;
    private string MyText { get; set; }
    private void IncrementCount()
    {
        currentCount++;
    }
}
